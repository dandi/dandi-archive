{
  "name": "e2e-tests",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "test": "jest --runInBand",
    "test-debug": "DEBUG=true jest --runInBand"
  },
  "dependencies": {
    "@faker-js/faker": "^7.5.0",
    "expect-puppeteer": "^6.1.1",
    "jest": "^29.1.2",
    "jest-puppeteer": "^6.1.1",
    "jest-puppeteer-vuetify": "0.1.3",
    "moment": "^2.29.4",
    "puppeteer": "^17.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.1",
    "babel-eslint": "^10.0.1",
    "eslint": "7.26.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.3.6"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "airbnb-base",
      "plugin:jest/all"
    ],
    "rules": {
      "jest/no-hooks": "off",
      "jest/prefer-expect-assertions": "off",
      "no-await-in-loop": "off",
      "no-restricted-syntax": "off"
    },
    "globals": {
      "page": true,
      "browser": true,
      "context": true,
      "jestPuppeteer": true
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  }
}
